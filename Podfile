# Uncomment the next line to define a global platform for your project
source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '8.0'

target 'YPProject' do
  # Uncomment the next line if you're using Swift or would like to use dynamic frameworks
  # use_frameworks!

  # Pods for YPProject

  pod 'SVProgressHUD'   # 菊花
  pod 'SDWebImage'      # 图片
  pod 'AFNetworking'    # 网络
  pod 'MJRefresh'       # 刷新
  pod 'MJExtension'     # 模型 数据之间的转换
  pod 'Masonry'         # 布局
  pod 'Reachability'    # 网络监听

#  pod 'YYCache'           # 缓存
#  pod 'IQKeyboardManager' # 键盘
#  pod 'LinqToObjectiveC'  # 数据查询

  pod 'MBProgressHUD'
  pod 'ReactiveObjC'      # 响应式 编程
  pod 'DZNEmptyDataSet'   # collectionview tableview emptyView
  pod 'SDCycleScrollView', '~>1.80'  # 滚动
  pod 'BlocksKit'         # block
  pod 'zhPopupController' # 弹窗 弹自定义View
  pod 'FLAnimatedImage'
  pod 'DACircularProgress'
  
  pod 'YPHttpTool',  :git =>'https://gitee.com/eMoision/YPHttpTool.git', :tag =>'0.0.2'

  target 'YPProjectTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'YPProjectUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
